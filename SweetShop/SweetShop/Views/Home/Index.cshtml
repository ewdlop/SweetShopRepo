@model HomeViewModel

@{
    Layout = "_Layout";
}
@await Html.PartialAsync("Carousel")
<h2>Pies of the week</h2>
<h4>Our weekly selection - just for you!</h4>

<div class="row">

    @foreach (var sweet in Model.SweetsOfTheWeek)
    {
        @await Html.PartialAsync("SweetOfTheWeekSummary", sweet)
    }

</div>
